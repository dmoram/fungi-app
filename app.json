{
  "expo": {
    "name": "Micelio",
    "slug": "fungi-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_ico.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/micelio_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E7EDC6"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_ico.png",
        "backgroundColor": "#3A0F76"
      },
      "package": "com.dmoram.micelioapp"
    },
    "web": {
      "favicon": "src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9369bdc1-f2ac-47b3-8328-6c4a6014d444"
      }
    },
    "plugins": [
      "expo-image-picker",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
